---
import { host } from "../../stuff";
const data = await fetch(`${host}/player/main/${Astro.params.smashId}`).then(res => res.json()).then(res => res);
console.log(data)
import Layout from "../../layouts/playerLayout.astro";
import Layout2 from '../../layouts/Layout.astro';
import $ from "jquery";
import { parse } from 'node-html-parser';
import { countryToAlpha2 } from "country-to-iso";
import flag from 'country-code-emoji';
let banner = `<div src="" class="opacity-25 w-full h-64 bg-gray-700"></div>`;
let pfp = `<img src="" class="w-28 h-28 rounded-lg uns" alt=${data.data.player.gamerTag}>`;
if (data.data.images.filter(img => img.type === "banner").length !== 0) banner = `<img
	src=${data.data.images.filter(img=> img.type === "banner")[0].url} class="opacity-25 w-full h-64">`;
if (data.data.images.filter(img => img.type === "profile").length !== 0) pfp = `<img src=${data.data.images.filter(img=>
img.type === "profile")[0].url} class="w-28 h-28 rounded-lg uns" style="display:unset !important"
alt=${data.data.player.gamerTag}>`;
---
<Layout2 title="Player Database">
	
	<div class="dark:bg-slate-950 overflow-y-hidden overflow-x-hidden flex flex-col">
		<div class="overflow-hidden rounded text-slate-500 shadow-slate-200">
			<figure class="relative">
				{parse(banner)}
				  <div class="md:flex md:justify-start md:text-start md:items-start justify-center items-center text-center absolute bottom-0 left-0 w-full p-6 text-white from-slate-900">
					{parse(pfp)}
					<div class="md:pl-2 place-content-end md:justify-start md:items-start md:text-start justify-center items-end text-center">
						<div class="md:justify-start md:items-start md:text-start justify-center items-center text-center">
							<span class="md:text-xl text-lg text-gray-600 dark:text-gray-400">{data.data.player.prefix}</span>
							<span class="md:justify-start md:items-start md:text-start justify-center items-center text-center flex md:text-3xl text-2xl dark:text-white">
								{data.data.country !== null ? <div title={data.data.country}>{flag(countryToAlpha2(data.data.country))}</div> : ""}
								{data.data.player.gamerTag}
							</span>
						</div>
						{data.data.pastNames.length !== 0 ?
						<div>
							<span class="md:text-xl text-lg text-gray-600 dark:text-gray-400">AKA </span>
							<span class="md:text-2xl text-xl text-gray-700 dark:text-gray-300">{data.data.pastNames.join(", ")}</span>
						</div> : ""
						}
						{data.data.bio !== null ? <span class="text-base text-gray-700 dark:text-gray-300">{data.data.bio}</span> : ""}
					</div>
				  </div>
			</figure>
