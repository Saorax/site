---
import { host } from "../../../stuff";
import Layout from "../../../layouts/PlayerLayout.jsx";
import Layout2 from '../../../layouts/Layout.astro';
import $ from "jquery";
import LoginButton from '../../../components/auth/butt.jsx';

let session = null;

if (typeof window !== "undefined") {
  const token = localStorage.getItem('accessToken');
  if (token) {
    session = JSON.parse(localStorage.getItem('user'));
  }
  if (session) {
    window.location.href = '/esports/autoseed/admin';
  }
}
---

<Layout2 title="StartGG Auto Seeding">
	<LoginButton client:load />
</Layout2>
